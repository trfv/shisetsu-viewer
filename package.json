{
  "name": "shisetsu-viewer",
  "version": "1.0.0",
  "private": false,
  "description": "Shisetsu Viewer",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/trfv/shisetsu-viewer/"
  },
  "license": "MIT",
  "author": "trfv",
  "dependencies": {
    "@apollo/client": "3.13.5",
    "@auth0/auth0-spa-js": "2.1.3",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "6.4.9",
    "@mui/material": "6.4.9",
    "@mui/x-date-pickers": "7.28.2",
    "@playwright/test": "1.51.1",
    "date-fns": "4.1.0",
    "graphql": "16.10.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "7.4.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-operations": "4.6.0",
    "@graphql-codegen/typescript-react-apollo": "4.3.2",
    "@storybook/addon-essentials": "8.6.11",
    "@storybook/react": "8.6.11",
    "@storybook/react-vite": "8.6.11",
    "@swc/core": "1.11.13",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.13.14",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitejs/plugin-react-swc": "3.8.1",
    "esbuild": "0.25.1",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "0.12.0",
    "globals": "16.0.0",
    "happy-dom": "17.4.4",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "rollup-plugin-visualizer": "5.14.0",
    "storybook": "8.6.11",
    "storycap": "5.0.1",
    "typescript": "5.8.2",
    "vite": "6.2.3",
    "vitest": "3.0.9",
    "web-vitals": "4.2.4"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "generate": "graphql-codegen --require dotenv/config --config ./packages/viewer/api/config.yml",
    "start": "npm run start:viewer",
    "start:viewer": "vite --port 3000",
    "start:scraper": "npx playwright test",
    "update:reservations": "node --env-file=.env .github/tools/updateReservations.mjs",
    "update:institutions": "node --env-file=.env .github/tools/updateInstitutions.mjs",
    "build": "tsc && vite build",
    "build:analyze": "vite build  --mode analyze",
    "serve": "vite preview --port 3000",
    "format:fix": "prettier --write",
    "lint:fix": "eslint --fix",
    "format:all": "npm run format:fix \"./packages/**/*.ts*\"",
    "lint:all": "npm run lint:fix \"./packages/**/*.ts*\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storycap": "storycap http://127.0.0.1:8080 --serverCmd \"npx http-server storybook-static -g -b\"",
    "test": "TZ=Asia/Tokyo vitest",
    "coverage": "TZ=Asia/Tokyo vitest run --coverage",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format:fix",
      "npm run lint:fix"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.37.0"
  }
}