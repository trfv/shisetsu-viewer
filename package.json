{
  "name": "shisetsu-viewer",
  "version": "1.0.0",
  "private": false,
  "description": "Shisetsu Viewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/trfv/shisetsu-viewer/"
  },
  "license": "MIT",
  "author": "trfv",
  "dependencies": {
    "@apollo/client": "3.3.11",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.3",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/pickers": "3.3.10",
    "date-fns": "2.19.0",
    "fontsource-roboto": "3.1.5",
    "graphql": "15.5.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.8.8",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "4.2.3",
    "web-vitals": "1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.17",
    "@babel/core": "7.12.17",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/node-logger": "6.1.21",
    "@storybook/react": "6.1.21",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.3",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.34",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "babel-loader": "8.1.0",
    "babel-plugin-i18next-extract": "0.8.2",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "i18next": "19.9.1",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "1.1.1"
  },
  "scripts": {
    "generate": "graphql-codegen --config ./src/api/config.yml",
    "i18next-extract": "NODE_ENV=development babel './src/**/*.{ts,tsx}'",
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "lint": "eslint --color --ignore-path .eslintignore \"./src/**/*.ts*\" --fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "i18next-extract",
        {
          "keySeparator": false,
          "nsSeparator": false,
          "locales": [
            "ja"
          ],
          "outputPath": "./src/locales/{{locale}}/{{ns}}.json"
        }
      ]
    ]
  }
}
