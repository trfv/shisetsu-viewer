{
  "name": "shisetsu-viewer",
  "version": "0.1.0",
  "private": false,
  "description": "Shisetsu Viewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/trfv/shisetsu-viewer/"
  },
  "license": "MIT",
  "author": "trfv",
  "dependencies": {
    "@apollo/client": "3.3.6",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/pickers": "3.2.10",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.6.0",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "date-fns": "2.16.1",
    "eslint": "7.16.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.3.0",
    "fontsource-roboto": "3.0.3",
    "graphql": "15.4.0",
    "i18next": "19.8.4",
    "prettier": "2.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.7.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "4.0.5",
    "web-vitals": "0.2.4"
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@graphql-codegen/cli": "1.19.2",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.10",
    "@graphql-codegen/typescript-react-apollo": "2.1.1",
    "@storybook/addon-actions": "6.1.6",
    "@storybook/addon-essentials": "6.1.6",
    "@storybook/addon-links": "6.1.6",
    "@storybook/node-logger": "6.1.6",
    "@storybook/preset-create-react-app": "3.1.5",
    "@storybook/react": "6.1.6",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.14",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/react-test-renderer": "17.0.0",
    "babel-loader": "8.1.0",
    "babel-plugin-i18next-extract": "0.8.2",
    "prettier-plugin-organize-imports": "1.1.1",
    "react-is": "17.0.1",
    "react-test-renderer": "17.0.1",
    "source-map-explorer": "2.5.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "generate": "graphql-codegen --config ./src/api/config.yml",
    "i18next-extract": "NODE_ENV=development babel './src/**/*.{ts,tsx}'",
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "lint": "eslint --color --ignore-path .eslintignore \"./src/**/*.ts*\" --fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "i18next-extract",
        {
          "locales": [
            "ja"
          ],
          "outputPath": "./src/locales/{{locale}}/{{ns}}.json"
        }
      ]
    ]
  }
}
