{
  "name": "shisetsu-viewer",
  "version": "1.0.0",
  "private": false,
  "description": "Shisetsu Viewer",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/trfv/shisetsu-viewer/"
  },
  "license": "MIT",
  "author": "trfv",
  "dependencies": {
    "@apollo/client": "3.4.17",
    "@auth0/auth0-spa-js": "1.19.2",
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@mui/icons-material": "5.1.0",
    "@mui/lab": "5.0.0-alpha.54",
    "@mui/material": "5.1.0",
    "date-fns": "2.25.0",
    "graphql": "16.0.1",
    "markdown-to-jsx": "7.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/typescript": "2.3.1",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.1",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/node-logger": "6.3.12",
    "@storybook/react": "6.3.12",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.27.0",
    "husky": "7.0.4",
    "lint-staged": "12.0.0",
    "prettier": "2.4.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "reg-keygen-git-hash-plugin": "0.11.1",
    "reg-notify-github-plugin": "0.11.1",
    "reg-publish-s3-plugin": "0.11.0",
    "reg-suit": "0.11.1",
    "rollup-plugin-visualizer": "5.5.2",
    "storybook-builder-vite": "0.1.6",
    "storycap": "3.1.0",
    "typescript": "4.4.4",
    "vite": "2.6.14",
    "web-vitals": "2.1.2"
  },
  "scripts": {
    "generate": "DOTENV_CONFIG_PATH=.env.local graphql-codegen -r dotenv/config --config ./src/api/config.yml",
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "build:analyze": "vite build  --mode analyze",
    "serve": "vite preview --port 3000",
    "format:fix": "prettier --ignore-path .prettierignore --write",
    "lint:fix": "eslint --ignore-path .eslintignore --fix",
    "format:all": "npm run format:fix \"./src/**/*.ts*\"",
    "lint:all": "npm run lint:fix \"./src/**/*.ts*\"",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "storycap": "storycap http://127.0.0.1:8080 --serverCmd \"npx http-server storybook-static\"",
    "reg-suit": "reg-suit run",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format:fix",
      "npm run lint:fix"
    ]
  }
}
