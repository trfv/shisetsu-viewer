{
  "name": "shisetsu-viewer",
  "version": "0.1.0",
  "private": false,
  "description": "Shisetsu Viewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/trfv/shisetsu-viewer/"
  },
  "license": "MIT",
  "author": "trfv",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.10.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.11",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "apollo-boost": "^0.4.9",
    "date-fns": "^2.15.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql": "^15.3.0",
    "i18next": "^19.6.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.6",
    "@graphql-codegen/cli": "^1.17.4",
    "@graphql-codegen/typescript": "^1.17.4",
    "@graphql-codegen/typescript-operations": "^1.17.4",
    "@graphql-codegen/typescript-react-apollo": "^1.17.4",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/node-logger": "^6.0.22",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.22",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-i18next-extract": "^0.7.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "generate": "graphql-codegen --config ./src/api/config.yml",
    "i18next-extract": "NODE_ENV=development babel './src/**/*.{ts,tsx}'",
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "lint": "eslint --color --ignore-path .eslintignore \"./src/**/*.ts*\" --fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "i18next-extract",
        {
          "locales": [
            "ja"
          ],
          "outputPath": "./src/locales/{{locale}}/{{ns}}.json"
        }
      ]
    ]
  }
}
