{
  "name": "shisetsu-viewer",
  "version": "1.0.0",
  "private": false,
  "description": "Shisetsu Viewer",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/trfv/shisetsu-viewer/"
  },
  "license": "MIT",
  "author": "trfv",
  "dependencies": {
    "@apollo/client": "3.12.5",
    "@auth0/auth0-spa-js": "2.1.3",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "6.3.1",
    "@mui/material": "6.3.1",
    "@mui/x-date-pickers": "7.23.6",
    "date-fns": "4.1.0",
    "graphql": "16.10.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "7.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/typescript": "4.1.2",
    "@graphql-codegen/typescript-operations": "4.4.0",
    "@graphql-codegen/typescript-react-apollo": "4.3.2",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/react": "8.4.7",
    "@storybook/react-vite": "8.4.7",
    "@swc/core": "1.10.7",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.10.5",
    "@types/react": "19.0.4",
    "@types/react-dom": "19.0.2",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "@vitejs/plugin-react-swc": "3.7.2",
    "esbuild": "0.25.0",
    "eslint": "9.18.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.37.3",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-storybook": "0.11.2",
    "globals": "15.14.0",
    "happy-dom": "16.5.3",
    "husky": "9.1.7",
    "lint-staged": "15.3.0",
    "prettier": "3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "rollup-plugin-visualizer": "5.14.0",
    "storybook": "8.4.7",
    "storycap": "5.0.1",
    "typescript": "5.7.3",
    "vite": "6.0.7",
    "vitest": "2.1.8",
    "web-vitals": "4.2.4"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "generate": "graphql-codegen --require dotenv/config --config ./src/api/config.yml",
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "build:analyze": "vite build  --mode analyze",
    "serve": "vite preview --port 3000",
    "format:fix": "prettier --write",
    "lint:fix": "eslint --fix",
    "format:all": "npm run format:fix \"./src/**/*.ts*\"",
    "lint:all": "npm run lint:fix \"./src/**/*.ts*\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storycap": "storycap http://127.0.0.1:8080 --serverCmd \"npx http-server storybook-static -g -b\"",
    "test": "TZ=Asia/Tokyo vitest",
    "coverage": "TZ=Asia/Tokyo vitest run --coverage",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format:fix",
      "npm run lint:fix"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.30.1"
  }
}
