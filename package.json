    "fontsource-roboto": "3.1.5",
    "graphql": "15.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "4.3.2",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.0",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@graphql-codegen/typescript-react-apollo": "2.2.5",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/node-logger": "6.2.9",
    "@storybook/react": "6.2.9",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26.0.23",
    "@types/node": "15.3.0",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.7",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/parser": "4.26.1",
    "babel-loader": "8.1.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.3.1",
    "prettier-plugin-organize-imports": "2.1.0"
  },
  "scripts": {
    "generate": "graphql-codegen --config ./src/api/config.yml",
    "start": "DISABLE_NEW_JSX_TRANSFORM=true react-scripts start",
    "build": "GENERATE_SOURCEMAP=false DISABLE_NEW_JSX_TRANSFORM=true react-scripts build",
    "lint": "eslint --color --ignore-path .eslintignore \"./src/**/*.ts*\" --fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
